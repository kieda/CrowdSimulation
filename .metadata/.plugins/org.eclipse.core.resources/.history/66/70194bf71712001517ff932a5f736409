package edu.cmu.cs464.p3.modulelang.ast

/**
 * @author zkieda
 */
abstract class ImportToken 
case class Star() extends ImportToken
case class Ident(id : String) extends ImportToken
  
object ImportToken{
  def star(){
    Star()
  }
  def ident(id : String){
    Ident(id);
  }
}
  
