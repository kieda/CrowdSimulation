package edu.cmu.cs464.p3.modulelang.ast;

import java.util.List;
import java.util.Map;
import java.util.function.BiFunction;

/**
 * @author zkieda
 */
public class Program {
    private List<ModuleImport> imports;
    private ParsedModule baseModule;
    public Program(List<ModuleImport> imports, ParsedModule baseModule) {
        this.imports = imports;
        this.baseModule = baseModule;
    }

    public ParsedModule getBaseModule() {
        return baseModule;
    }

    public List<ModuleImport> getImports() {
        return imports;
    }
}
