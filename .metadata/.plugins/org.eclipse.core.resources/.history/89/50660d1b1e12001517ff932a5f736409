package edu.cmu.cs464.p3.modulelang.ast;

import java.util.List;
import java.util.function.BiFunction;
import java.util.stream.Collectors;

/**
 * @author zkieda
 */
public class ParsedModule {
    private JavaRef baseClass;
    private List<ParsedModule> children;
    
    public ParsedModule(JavaRef baseClass, List<ParsedModule> children) {
        this.baseClass = baseClass;
        this.children = children;
    }

    public JavaRef getBaseClass() {
        return baseClass;
    }

    public List<ParsedModule> getChildren() {
        return children;
    }

    @Override
    public String toString() {
        return baseClass + "\nhas\n" + children.stream().map(Object::toString).collect(Collectors.joining("\n")) + "\nend";
    }
    
}
